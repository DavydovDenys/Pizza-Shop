<h1>Order details</h1>
<br/>
<table class="table">
	<thead>
   		<tr>
      
	    	<th scope="col">Name</th>
	    	<th scope="col">Price</th>
	    	<th scope="col">Qty</th>
	    	    
    	</tr>  
	</thead>

	<tbody>
		<% @items.each do |item| %>
    	<tr>
	    
		    <td><%= item[0].title %></td>
		    <td><%= item[0].price %></td>
		    <td><%= item[1] %></td>

    	</tr>
	</tbody>
	<% end %>
</table>



